{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2015",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "strict": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "dist/tmp"
  },
  "exclude": ["node_modules", "dist"]
}
