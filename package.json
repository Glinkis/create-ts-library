{
  "name": "jslib-utils",
  "author": "Victor Glind√•s",
  "version": "1.30.0",
  "description": "A modular javascript utility library.",
  "keywords": [
    "animation",
    "easing",
    "array",
    "color",
    "dom",
    "interactivity",
    "math",
    "splines",
    "vector",
    "library",
    "javascript",
    "typescript",
    "es6",
    "modular",
    "utilities"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": ["dist", "lib", "es"],
  "scripts": {
    "lint": "tslint src",
    "prebuild": "rimraf dist && rimraf lib && rimraf es",
    "build":
      "tsc && tsc --module commonjs -outDir lib && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal src && touch docs/.nojekyll",
    "prettier":
      "(prettier --write \"src/**/*.ts\") & (prettier --write \"test/**/*.ts\")",
    "test": "(./node_modules/karma/bin/karma start) | (karma start)",
    "test:once":
      "(./node_modules/karma/bin/karma start --single-run) | (karma start --single-run)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Glinkis@github.com/Glinkis/jslib.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/jslib/issues"
  },
  "homepage": "https://github.com/Glinkis/jslib#readme",
  "devDependencies": {
    "@types/chai": "^4.0.7",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.54",
    "@types/webpack": "^3.8.1",
    "ant-design": "^1.0.0",
    "antd": "^3.0.0",
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^3.0.8",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill-find": "0.0.1",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "touch-cli": "0.0.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-external-module-name": "^1.0.10",
    "typescript": "^2.6.2",
    "webpack": "^3.9.1"
  }
}
