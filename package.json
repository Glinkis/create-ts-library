{
  "name": "jslib",
  "author": "Victor Glind√•s",
  "version": "1.0.0",
  "description": "A modular javascript utility library.",
  "main": "./src/index.js",
  "scripts": {
    "build": "(documentation build -o docs/src/api.json) & (webpack -p)",
    "start": "run-p docs server",
    "server": "webpack-dev-server -d --hot",
    "docs": "documentation build --watch -o docs/src/api.json",
    "pretty": "(prettier --write \"src/**/*.js\") & (prettier --write \"docs/src/**/*.js\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Glinkis@github.com/Glinkis/jslib.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/jslib/issues"
  },
  "homepage": "https://github.com/Glinkis/jslib#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "documentation": "^4.0.0-rc.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3"
  }
}
