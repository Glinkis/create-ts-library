{
  "name": "jslib-utils",
  "author": "Victor Glind√•s",
  "version": "1.36.0",
  "description": "A modular javascript utility library.",
  "keywords": [
    "animation",
    "easing",
    "array",
    "color",
    "dom",
    "interactivity",
    "math",
    "splines",
    "vector",
    "library",
    "javascript",
    "typescript",
    "es6",
    "modular",
    "utilities"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": ["dist", "lib", "es"],
  "scripts": {
    "lint": "tslint ./",
    "prebuild": "rimraf dist && rimraf lib && rimraf es",
    "build": "tsc && tsc --module commonjs -outDir lib && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal src",
    "prettier": "prettier --write \"./*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:watch": "karma start",
    "test": "npm run test:watch -- --single-run"
  },
  "pre-commit": "lint, test",
  "repository": {
    "type": "git",
    "url": "git+https://Glinkis@github.com/Glinkis/jslib.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/jslib/issues"
  },
  "homepage": "https://github.com/Glinkis/jslib#readme",
  "devDependencies": {
    "@types/chai": "^4.0.10",
    "@types/mocha": "^2.2.45",
    "@types/node": "^8.5.2",
    "@types/webpack": "^3.8.2",
    "chai": "^4.1.2",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.9",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.3",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-external-module-name": "^1.0.10",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
