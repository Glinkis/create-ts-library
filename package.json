{
  "name": "jslib-utils",
  "author": "Victor Glind√•s",
  "version": "1.27.3",
  "description": "A modular javascript utility library.",
  "keywords": [
    "animation",
    "easing",
    "array",
    "color",
    "dom",
    "interactivity",
    "math",
    "splines",
    "vector",
    "library",
    "javascript",
    "es6",
    "modular",
    "utilities"
  ],
  "main": "dist/jslib.js",
  "module": "dist/jslib.js",
  "typings": "dist/types/jslib.d.ts",
  "files": ["dist"],
  "scripts": {
    "prebuild": "rimraf dist",
    "build":
      "tsc && rollup -c rollup.config.ts && rimraf compiled && typedoc --out docs --target es6 --theme minimal src",
    "start": "tsc -w & rollup -c rollup.config.ts -w",
    "prettier":
      "(prettier --write \"src/**/*.ts\") & (prettier --write \"test/**/*.js\") & (prettier --write \"test/**/*.ts\")",
    "test": "(./node_modules/karma/bin/karma start) | (karma start)",
    "test:once":
      "(./node_modules/karma/bin/karma start --single-run) | (karma start --single-run)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Glinkis@github.com/Glinkis/jslib.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Glinkis/jslib/issues"
  },
  "homepage": "https://github.com/Glinkis/jslib#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.32",
    "chai": "^4.1.2",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^3.0.8",
    "mocha": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "phantomjs": "^2.1.7",
    "phantomjs-polyfill-find": "0.0.1",
    "prettier": "^1.7.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.2"
  }
}
