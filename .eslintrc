{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "import"
  ],
  "rules": {
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "arrow-body-style": "off",
    "no-console": "off",
    "radix": "off",
    "no-unused-expressions": "off",
    "import/prefer-default-export": "off",
    "jsx-a11y/href-no-hash": "off"
  },
  "env": {
    "browser": true,
    "mocha": true
  }
}
